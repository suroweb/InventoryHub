{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft.AspNetCore": "Information",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "Cors": {
    "AllowedOrigins": [
      "http://localhost:5173",
      "https://localhost:5173",
      "http://localhost"
    ]
  },
  "Caching": {
    "OutputCacheDurationMinutes": 1
  },
  "RateLimiting": {
    "PermitLimit": 1000,
    "WindowSeconds": 60
  },
  "DetailedErrors": true
}
